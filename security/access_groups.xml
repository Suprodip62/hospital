            <field name="name">Approver</field>
            <field name="category_id" ref="hospital.acess_category_hospital"/>
            <field name="implied_ids" eval="[(4,ref('hospital.acess_category_hospital_user'))]"/>
        </record>

        <record id="acess_category_hospital_admin" model="res.groups">
            <field name="name">Administrator</field>
            <field name="category_id" ref="hospital.acess_category_hospital"/>
            <field name="implied_ids" eval="[(4,ref('hospital.acess_category_hospital_approver'))]"/>
            <field name="users" eval="[(4,ref('base.user_admin'))]"/>
        </record>

